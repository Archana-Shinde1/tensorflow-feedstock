From 01327266f953fd4d4b11d510a7354da50b01c2a8 Mon Sep 17 00:00:00 2001
From: ArchanaShinde1 <Archana.Shinde1@ibm.com>
Date: Wed, 5 Jun 2024 06:50:23 +0000
Subject: [PATCH] include cc in pip package

---
 tensorflow/tools/pip_package/BUILD    | 7 +++++++
 tensorflow/tools/pip_package/setup.py | 2 ++
 2 files changed, 9 insertions(+)

diff --git a/tensorflow/tools/pip_package/BUILD b/tensorflow/tools/pip_package/BUILD
index 91be8d17531..0241765d3e2 100644
--- a/tensorflow/tools/pip_package/BUILD
+++ b/tensorflow/tools/pip_package/BUILD
@@ -114,6 +114,13 @@ COMMON_PIP_DEPS = [
     "//tensorflow/dtensor/python/tests:test_util",
     "//tensorflow/lite/python:tflite_convert",
     "//tensorflow/lite/toco/python:toco_from_protos",
+    "//tensorflow/c:headers",
+    "//tensorflow/c/eager:headers",
+    "//tensorflow/cc:cc_ops_headers",
+    "//tensorflow/cc:cc_gen_headers",
+    "//tensorflow/cc:cc_framework_headers",
+    "//tensorflow:libtensorflow_cc.so",
+    "//tensorflow:libtensorflow.so",
     "//tensorflow/lite/tools:visualize",
     "//tensorflow/python/autograph/core:test_lib",
     "//tensorflow/python/autograph/impl/testing:pybind_for_testing",
diff --git a/tensorflow/tools/pip_package/setup.py b/tensorflow/tools/pip_package/setup.py
index cdec9d3a783..2a27e3f424a 100644
--- a/tensorflow/tools/pip_package/setup.py
+++ b/tensorflow/tools/pip_package/setup.py
@@ -384,6 +384,8 @@ headers = (
     + list(find_files('*.h', 'tensorflow/include/external/ducc/google'))
     + list(find_files('*', 'tensorflow/include/external/eigen_archive'))
     + list(find_files('*.h', 'tensorflow/include/external/ml_dtypes'))
+    + list(find_files('*', 'tensorflow/c'))
+    + list(find_files('*', 'tensorflow/cc'))
 )
 
 # Quite a lot of setup() options are different if this is a collaborator package
-- 
